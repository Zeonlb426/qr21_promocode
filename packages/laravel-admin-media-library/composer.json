{
    "name": "nicklasos/laravel-admin-media-library",
    "description": "Media-library extension for laravel-admin",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "extension",
        "media-library",
        "media"
    ],
    "homepage": "https://github.com/nicklasos/laravel-admin-media-library",
    "license": "MIT",
    "version": "v1.0",
    "authors": [
        {
            "name": "Nicklasos",
            "email": "nicklasos@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6",
        "spatie/laravel-medialibrary": "^8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Nicklasos\\LaravelAdmin\\MediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nicklasos\\LaravelAdmin\\MediaLibrary\\MediaLibraryServiceProvider"
            ]
        }
    }
}
